{
  "$schema": "http://json-schema.org/schema",
  "id": "SchematicsSchematicSchema",
  "title": "Schematic Options Schema",
  "definitions": {
    "includeItem": {
      "$id": "#includeItem",
      "type": "string",
      "enum": [
        "lint-staged",
        "commitlint",
        "githooks",
        "renovate",
        "jest",
        "prettier"
      ]
    }
  },
  "type": "object",
  "properties": {
    "include": {
      "type": "array",
      "items": {
        "$ref": "#includeItem"
      },
      "description": "The project tools want add"
    },
    "cwd": {
      "type": "string",
      "description": "Specify the schematic execute cwd relative to current cwd"
    },
    "interactive": {
      "type": "boolean",
      "default": true,
      "description": "Ask some option interactively"
    },
    "jest-react": {
      "type": "boolean",
      "default": true,
      "description": "Add react support to jest"
    }
  },
  "required": []
}
