{
  "name": "@c4/g",
  "version": "0.3.0",
  "description": "A blank schematics",
  "scripts": {
    "g": "schematics",
    "build-schema-ts": "ts-node ./scripts/generate-schema-ts.ts src/**/schema.json",
    "watch-schema-ts": "yarn build-schema-ts && trap 'exit' INT; while true; do ls src/**/schema.json | entr -d yarn build-schema-ts /_; done",
    "build-ts": "tsc -p tsconfig.json",
    "watch-ts": "yarn build-ts --watch",
    "clean-output": "rm -f ./src/**/*{.d.ts,.d.ts.map,.js,.js.map}",
    "build": "yarn build-schema-ts && yarn build-ts",
    "watch": "concurrently 'yarn:watch-schema-ts' 'yarn:watch-ts'",
    "prepublishOnly": "yarn clean-output && yarn build",
    "test": "yarn build-schema-ts && NODE_ENV=test jest"
  },
  "prettier": "@c4605/toolconfs/prettierrc",
  "keywords": [
    "schematics"
  ],
  "author": "c4605 <bolasblack@gmail.com>",
  "license": "MIT",
  "schematics": "./src/collection.json",
  "dependencies": {
    "@angular-devkit/core": "^8.1.2",
    "@angular-devkit/schematics": "^8.1.2",
    "@c4605/toolconfs": "^4.4.0",
    "@types/inquirer": "^6.0.3",
    "@types/node": "^12.6.8",
    "chalk": "^2.4.2",
    "immer": "^3.2.0",
    "inquirer": "^6.5.0",
    "js-yaml": "^3.13.1",
    "typescript": "~3.5.3"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^8.1.0",
    "@types/chalk": "^2.2.0",
    "@types/jasmine": "^3.3.15",
    "@types/jest": "^24.0.15",
    "@types/js-yaml": "^3.12.1",
    "@typescript-eslint/eslint-plugin": "^2.0.0",
    "@typescript-eslint/parser": "^2.0.0",
    "commitlint": "^8.1.0",
    "concurrently": "^4.1.1",
    "coveralls": "^3.0.5",
    "eslint": "^6.1.0",
    "eslint-config-prettier": "^6.0.0",
    "git-hook-pure": "^1.2.2",
    "jest": "^24.9.0",
    "jest-cli": "^24.9.0",
    "lint-staged": "^9.2.0",
    "prettier": "^1.18.2",
    "quicktype": "^15.0.198",
    "ts-jest": "^24.0.2",
    "ts-node": "^8.3.0"
  },
  "resolutions": {
    "**/rxjs": "^6.4.0"
  }
}
