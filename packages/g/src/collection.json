{
  "$schema": "../node_modules/@angular-devkit/schematics/collection-schema.json",
  "schematics": {
    "react-new": {
      "description": "Create a new react project",
      "factory": "./react-new/index#main",
      "schema": "./react-new/schema.json"
    },
    "node-new": {
      "description": "Create a new node project",
      "factory": "./node-new/index#main",
      "schema": "./node-new/schema.json"
    },
    "lib-new": {
      "description": "Create a new library project",
      "factory": "./lib-new/index#main",
      "schema": "./lib-new/schema.json"
    },
    "schematic": {
      "description": "Add new schematic to c4g",
      "factory": "./schematic/index#main",
      "schema": "./schematic/schema.json"
    },
    "proj-tools": {
      "description": "Add project tools to project",
      "factory": "./proj-tools/index#main",
      "schema": "./proj-tools/schema.json"
    }
  }
}
