{
  "name": "@c4/g",
  "version": "0.5.0",
  "description": "A blank schematics",
  "scripts": {
    "g": "schematics",
    "build-schema-ts": "ts-node ./scripts/generate-schema-ts.ts src/**/schema.json",
    "watch-schema-ts": "yarn build-schema-ts && trap 'exit' INT; while true; do ls src/**/schema.json | entr -d yarn build-schema-ts /_; done",
    "build-ts": "tsc -p tsconfig.json",
    "watch-ts": "yarn build-ts --watch",
    "clean-output": "rm -f ./src/**/*{.d.ts,.d.ts.map,.js,.js.map}",
    "build": "yarn build-schema-ts && yarn build-ts",
    "watch": "concurrently 'yarn:watch-schema-ts' 'yarn:watch-ts'",
    "prepublishOnly": "yarn clean-output && yarn build",
    "test": "yarn build-schema-ts && NODE_ENV=test jest",
    "test:ci": "yarn test --coverage --passWithNoTests"
  },
  "prettier": "@c4605/toolconfs/prettierrc",
  "keywords": [
    "schematics"
  ],
  "author": "c4605 <bolasblack@gmail.com>",
  "license": "MIT",
  "schematics": "./src/collection.json",
  "dependencies": {
    "@angular-devkit/core": "^10.0.0",
    "@angular-devkit/schematics": "^10.0.0",
    "@c4605/schematic-utils": "^0.1.1",
    "@c4605/toolconfs": "^5.0.0",
    "@types/inquirer": "^6.5.0",
    "@types/node": "^12.7.2",
    "@types/semver": "^7.0.0",
    "chalk": "^4.0.0",
    "immer": "^6.0.0",
    "inquirer": "^7.0.0",
    "js-yaml": "^3.13.1",
    "package-json": "^6.5.0",
    "semver": "^7.0.0",
    "typescript": "~3.9.0"
  },
  "devDependencies": {
    "@types/chalk": "^2.2.0",
    "@types/jest": "^25.0.0",
    "@types/js-yaml": "^3.12.1",
    "concurrently": "^5.0.0",
    "coveralls": "^3.0.6",
    "jest": "^26.0.0",
    "jest-cli": "^26.0.0",
    "quicktype": "^15.0.199",
    "ts-jest": "^26.0.0",
    "ts-node": "^8.3.0"
  }
}
